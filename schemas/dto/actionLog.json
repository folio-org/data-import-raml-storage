{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Action log schema",
  "javaType": "org.folio.rest.jaxrs.model.ActionLog",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "entityType": {
      "description": "Type of entity",
      "type": "string"
    },
    "actionType": {
      "description": "Type of action",
      "type": "string"
    },
    "totalCompleted": {
      "description": "Number of successfully processed entities",
      "type": "integer"
    },
    "totalFailed": {
      "description": "Number of failed processing",
      "type": "integer"
    }
  },
  "required": [
    "entityType",
    "actionType",
    "totalCompleted",
    "totalFailed"
  ]
}
