{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Profile schema",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identifier",
      "type": "string"
    },
    "name": {
      "description": "Profile caption",
      "type": "string"
    },
    "state": {
      "description": "Current state of the job profile",
      "type": "string",
      "enum": ["DIRTY", "COMMITTED"]
    }
  },
  "required": [
    "id",
    "name",
    "state"
  ]
}
