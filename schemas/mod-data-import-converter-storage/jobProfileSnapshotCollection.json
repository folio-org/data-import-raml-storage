{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of Profile snapshots",
  "type": "object",
  "properties": {
    "jobProfileSnapshots": {
      "description": "Profile snapshot collection",
      "type": "array",
      "id": "snapshotList",
      "items": {
        "type": "object",
        "$ref": "jobProfileSnapshot.json"
      }
    },
    "totalRecords": {
      "description": "Total number of Profile snapshots",
      "type": "integer"
    }
  },
  "required": [
    "jobProfileSnapshots",
    "totalRecords"
  ]
}
